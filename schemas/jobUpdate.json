{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://our.jobs.com/jobNew.schema.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "title",
        "salary",
        "equity",
        "company_handle"
    ],
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "new"
            ]
        },
        "salary": {
            "type": "integer",
            "default": 0,
            "title": "The salary Schema",
            "examples": [
                99999
            ]
        },
        "equity": {
            "type": "numeric",
            "default": 0.0,
            "title": "The equity Schema",
            "examples": [
                0.05
            ]
        }
    },
    "examples": [{
        "id": 1,
        "title": "new",
        "salary": 99999,
        "equity": 0.05,
        "company_handle": "c1"
    }]
}